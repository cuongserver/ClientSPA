{
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"extends": [
		"plugin:react/recommended"
	],
	"rules": {
		"@typescript-eslint/no-explicit-any": [
			"error"
		],
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": [
			"error"
		]
	}
}